<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="module.SystemPanel">
   <children>
      <AnchorPane prefHeight="80.0" prefWidth="150.0" style="-fx-border-color: #000;">
         <children>
            <ImageView fitHeight="50.0" fitWidth="50.0" layoutX="5.0" pickOnBounds="true" preserveRatio="true" y="5.0">
               <image>
                  <Image url="@../../Material/timer.png" />
               </image>
            </ImageView>
            <Label fx:id="time" alignment="CENTER" layoutX="50.0" layoutY="15.0" prefHeight="50.0" prefWidth="100.0" text="Label">
               <font>
                  <Font size="30.0" />
               </font>
            </Label>
            <Label fx:id="time1" alignment="CENTER" layoutX="5.0" layoutY="60.0" prefHeight="15.0" prefWidth="50.0" text="Time">
               <font>
                  <Font size="15.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
      <AnchorPane layoutX="150.0" prefHeight="80.0" prefWidth="150.0" style="-fx-border-color: #000;">
         <children>
            <ImageView fitHeight="50.0" fitWidth="50.0" layoutX="5.0" pickOnBounds="true" preserveRatio="true" y="5.0">
               <image>
                  <Image url="@../../Material/scale_up.png" />
               </image>
            </ImageView>
            <Label fx:id="scale" alignment="CENTER" layoutX="50.0" layoutY="15.0" prefHeight="50.0" prefWidth="100.0" text="Label">
               <font>
                  <Font size="30.0" />
               </font>
            </Label>
            <Label fx:id="time11" alignment="CENTER" layoutX="5.0" layoutY="60.0" prefHeight="15.0" prefWidth="50.0" text="Scale">
               <font>
                  <Font size="15.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
